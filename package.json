{
  "name": "lynxbot-neo",
  "version": "0.2.0",
  "description": "A simple discord.js bot template written in nodejs",
  "main": "lib/bot.js",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "debug": "nodemon --inspect .",
    "start": "node .",
    "setup": "node ./lib/utils/setup.js"
  },
  "keywords": [
    "bot",
    "discordjs",
    "nodejs",
    "framework"
  ],
  "author": "NovaLynxie",
  "license": "ISC",
  "dependencies": {
    "discord.js": "^14.16.3",
    "dotenv": "^16.4.7",
    "sequelize": "^6.37.5",
    "sqlite3": "^5.1.7",
    "toml": "^3.0.0",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "eslint": "^9.17.0",
    "nodemon": "^3.1.9"
  },
  "nodemonConfig": {
    "delay": 5000,
    "ignore": [
      "lib/commands/*",
      "lib/events/*",
      "logs/*"
    ]
  }
}
